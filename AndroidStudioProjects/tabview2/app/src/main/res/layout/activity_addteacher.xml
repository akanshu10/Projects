<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".college.addteacherActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_marginBottom="10dp"
        android:background="@color/back">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:id="@+id/addmissionform"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="10dp"
            android:gravity="center"
            android:background="#F8F3F3"
            android:text="Registration Form"
            android:textSize="30dp"
            android:textStyle="bold"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_below="@+id/addmissionform"
            android:layout_marginEnd="10dp"
            android:gravity="center"
            android:background="#F8F3F3"
            android:text="Part (I)"
            android:textSize="30dp"
            android:textStyle="bold"/>


    </RelativeLayout>
    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentTop="false"
        android:background="@color/back"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="135dp">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="7dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/parsenaldetailstext"
                        android:text="Parsonal details"
                        android:textSize="22sp"
                        android:textColor="#000000"
                        android:textStyle="bold"

                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:padding="5dp"
                        />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="10dp"
                        android:orientation="horizontal">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardview1"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:layout_marginRight="5dp"
                            android:orientation="vertical"
                            android:gravity="center">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginTop="20dp"
                                >

                                <TextView
                                    android:id="@+id/tfullnametext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Full Name"
                                    android:textSize="17sp"
                                    android:textColor="#000000"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />

                                <EditText
                                    android:id="@+id/tfullnameedit"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="#000000"
                                    android:textSize="17sp"
                                    android:layout_below="@+id/tfullnametext"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginTop="0dp"
                                    android:layout_marginLeft="20dp" />
                                <TextView
                                    android:id="@+id/tfathernametext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Father Name"
                                    android:textSize="17sp"
                                    android:layout_below="@+id/tfullnameedit"
                                    android:textColor="#000000"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />

                                <EditText
                                    android:id="@+id/tfathernameedit"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="#000000"
                                    android:textSize="17sp"
                                    android:layout_below="@+id/tfathernametext"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />

                                <TextView
                                    android:id="@+id/tMothernametext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Mother Name"
                                    android:textSize="17sp"
                                    android:textColor="#000000"
                                    android:layout_below="@+id/tfathernameedit"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />

                                <EditText
                                    android:id="@+id/tmothernameedit"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="#000000"
                                    android:layout_below="@+id/tMothernametext"
                                    android:textSize="17sp"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />

                                <TextView
                                    android:id="@+id/tdobtext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Date of Birth"
                                    android:textSize="17sp"
                                    android:textColor="#000000"
                                    android:layout_below="@+id/tmothernameedit"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />

                                <EditText
                                    android:id="@+id/tdobedit"
                                    android:layout_below="@+id/tdobtext"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="#000000"
                                    android:textSize="17sp"
                                    android:inputType="date"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />
                                <TextView
                                    android:id="@+id/tgendertext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Gender"
                                    android:textSize="17sp"
                                    android:textColor="#000000"
                                    android:layout_marginStart="20dp"
                                    android:layout_below="@+id/tdobedit"
                                    android:layout_marginLeft="20dp" />

                                <EditText
                                    android:id="@+id/tgenderedit"
                                    android:layout_below="@+id/tgendertext"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="#000000"
                                    android:textSize="17sp"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />
                                <TextView
                                    android:id="@+id/tcatogrytext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Category"
                                    android:textSize="17sp"
                                    android:textColor="#000000"
                                    android:layout_marginStart="20dp"
                                    android:layout_below="@+id/tgenderedit"
                                    android:layout_marginLeft="20dp" />

                                <EditText
                                    android:id="@+id/tcategoryedit"
                                    android:layout_below="@+id/tcatogrytext"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="#000000"
                                    android:textSize="17sp"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />
                                <TextView
                                    android:id="@+id/treligiontext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Religion"
                                    android:textSize="17sp"
                                    android:textColor="#000000"
                                    android:layout_marginStart="20dp"
                                    android:layout_below="@+id/tcategoryedit"
                                    android:layout_marginLeft="20dp" />

                                <EditText
                                    android:id="@+id/treligionedit"
                                    android:layout_below="@+id/treligiontext"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="#000000"
                                    android:textSize="17sp"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />

                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>

                    </LinearLayout>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/communicationdetailstext"
                        android:text="Communication details"
                        android:textSize="22sp"
                        android:textColor="#000000"
                        android:textStyle="bold"

                        android:layout_marginTop="3dp"
                        android:layout_marginBottom="5dp"
                        android:padding="5dp"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="10dp"
                        android:orientation="horizontal">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardview2"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:layout_marginRight="5dp"
                            android:orientation="vertical"
                            android:gravity="center">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginTop="20dp"
                                >

                                <TextView
                                    android:id="@+id/tvillagetext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Village/House no./Colony"
                                    android:textSize="17sp"
                                    android:textColor="#000000"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />

                                <EditText
                                    android:id="@+id/tvillageedit"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="#000000"
                                    android:textSize="17sp"
                                    android:layout_below="@+id/tvillagetext"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginTop="0dp"
                                    android:layout_marginLeft="20dp" />
                                <TextView
                                    android:id="@+id/tposttext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Post Office"
                                    android:textSize="17sp"
                                    android:layout_below="@+id/tvillageedit"
                                    android:textColor="#000000"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />

                                <EditText
                                    android:id="@+id/tpostedit"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="#000000"
                                    android:textSize="17sp"
                                    android:layout_below="@+id/tposttext"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />

                                <TextView
                                    android:id="@+id/tcitytext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="City"
                                    android:textSize="17sp"
                                    android:textColor="#000000"
                                    android:layout_below="@+id/tpostedit"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />

                                <EditText
                                    android:id="@+id/tcityedit"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="#000000"
                                    android:layout_below="@+id/tcitytext"
                                    android:textSize="17sp"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />

                                <TextView
                                    android:id="@+id/tdisttext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="District"
                                    android:textSize="17sp"
                                    android:textColor="#000000"
                                    android:layout_below="@+id/tcityedit"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />

                                <EditText
                                    android:id="@+id/tdistedit"
                                    android:layout_below="@+id/tdisttext"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="#000000"
                                    android:textSize="17sp"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />
                                <TextView
                                    android:id="@+id/tstatetext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="State"
                                    android:textSize="17sp"
                                    android:textColor="#000000"
                                    android:layout_marginStart="20dp"
                                    android:layout_below="@+id/tdistedit"
                                    android:layout_marginLeft="20dp" />

                                <EditText
                                    android:id="@+id/tstateedit"
                                    android:layout_below="@+id/tstatetext"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="#000000"
                                    android:textSize="17sp"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />
                                <TextView
                                    android:id="@+id/tpintext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Pin Code"
                                    android:textSize="17sp"
                                    android:textColor="#000000"
                                    android:layout_marginStart="20dp"
                                    android:layout_below="@+id/tstateedit"
                                    android:layout_marginLeft="20dp" />

                                <EditText
                                    android:id="@+id/tpinedit"
                                    android:layout_below="@+id/tpintext"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="#000000"
                                    android:textSize="17sp"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />
                                <TextView
                                    android:id="@+id/tmobiletext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Mobile no."
                                    android:textSize="17sp"
                                    android:textColor="#000000"
                                    android:layout_marginStart="20dp"
                                    android:layout_below="@+id/tpinedit"
                                    android:layout_marginLeft="20dp" />

                                <EditText
                                    android:id="@+id/tMobileedit"
                                    android:layout_below="@+id/tmobiletext"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="#000000"
                                    android:textSize="17sp"
                                    android:inputType="number"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />
                                <TextView
                                    android:id="@+id/temailtext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Email"
                                    android:textSize="17sp"
                                    android:textColor="#000000"
                                    android:layout_marginStart="20dp"
                                    android:layout_below="@+id/tMobileedit"
                                    android:layout_marginLeft="20dp" />

                                <EditText
                                    android:id="@+id/temailedit"
                                    android:layout_below="@+id/temailtext"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="#000000"
                                    android:textSize="17sp"
                                    android:inputType="textEmailAddress"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />

                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>

                    </LinearLayout>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/qualifyingdetailstext"
                        android:text="Higher Qualification Details"
                        android:textSize="22sp"
                        android:textColor="#000000"
                        android:textStyle="bold"

                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:padding="5dp"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="10dp"
                        android:orientation="horizontal">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardview3"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:layout_marginRight="5dp"
                            android:orientation="vertical"
                            android:gravity="center">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginTop="20dp"
                                >

                                <TextView
                                    android:id="@+id/qtschooltext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="College/Institute Name"
                                    android:textSize="17sp"
                                    android:textColor="#000000"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />

                                <EditText
                                    android:id="@+id/qtschooledit"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="#000000"
                                    android:textSize="17sp"
                                    android:layout_below="@+id/qtschooltext"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginTop="0dp"
                                    android:layout_marginLeft="20dp" />
                                <TextView
                                    android:id="@+id/qtschooladdtext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="College/institute Address"
                                    android:textSize="17sp"
                                    android:layout_below="@+id/qtschooledit"
                                    android:textColor="#000000"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />
                                <EditText
                                    android:id="@+id/qtschooladdedit"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="#000000"
                                    android:textSize="17sp"
                                    android:layout_below="@+id/qtschooladdtext"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginTop="0dp"
                                    android:layout_marginLeft="20dp" />

                                <TextView
                                    android:id="@+id/qtsujecttobtainedmarkstext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Total Obtained Marks"
                                    android:textSize="17sp"
                                    android:layout_below="@+id/qtschooladdedit"
                                    android:textColor="#000000"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />
                                <EditText
                                    android:id="@+id/qtsubjecttobtainedmarkedit"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="#000000"
                                    android:textSize="17sp"
                                    android:inputType="number"
                                    android:layout_below="@+id/qtsujecttobtainedmarkstext"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginTop="0dp"
                                    android:layout_marginLeft="20dp" />

                                <TextView
                                    android:id="@+id/qtsujecttmaxmarkstext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Total Maximum Marks"
                                    android:textSize="17sp"
                                    android:layout_below="@+id/qtsubjecttobtainedmarkedit"
                                    android:textColor="#000000"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp" />
                                <EditText
                                    android:id="@+id/qtsubjecttmaxmarkedit"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textColor="#000000"
                                    android:textSize="17sp"
                                    android:inputType="number"
                                    android:layout_below="@+id/qtsujecttmaxmarkstext"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginTop="0dp"
                                    android:layout_marginLeft="20dp" />


                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>

                    </LinearLayout>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/qualifyingsubjectstext"
                        android:text="Qualifying Subjects Details"
                        android:textSize="22sp"
                        android:textColor="#000000"
                        android:textStyle="bold"
                        android:layout_marginBottom="5dp"
                        android:layout_marginTop="5dp"
                        android:padding="5dp"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="10dp"
                        android:orientation="horizontal">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardview4"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:layout_marginRight="5dp"
                            android:orientation="vertical"
                            android:gravity="center">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginTop="20dp">

                                <TextView
                                    android:id="@+id/qsubjecttext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp"
                                    android:text="Subject Name"
                                    android:textColor="#000000"
                                    android:textSize="17sp" />

                                <EditText
                                    android:id="@+id/qsubjectedit"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/qsubjecttext"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp"
                                    android:layout_marginTop="0dp"
                                    android:textColor="#000000"
                                    android:textSize="17sp" />

                                <TextView
                                    android:id="@+id/qsujectobtainedmarkstext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/qsubjectedit"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp"
                                    android:text="Obtained Marks"
                                    android:textColor="#000000"
                                    android:textSize="17sp" />

                                <EditText
                                    android:id="@+id/qsubjectobtainedmarkedit"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/qsujectobtainedmarkstext"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp"
                                    android:layout_marginTop="0dp"
                                    android:inputType="number"
                                    android:textColor="#000000"
                                    android:textSize="17sp" />

                                <TextView
                                    android:id="@+id/qsujectmaxmarkstext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/qsubjectobtainedmarkedit"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp"
                                    android:text="Maximum Marks"
                                    android:textColor="#000000"
                                    android:textSize="17sp" />

                                <EditText
                                    android:id="@+id/qsubjectmaxmarkedit"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/qsujectmaxmarkstext"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp"
                                    android:layout_marginTop="0dp"
                                    android:inputType="number"
                                    android:textColor="#000000"
                                    android:textSize="17sp" />

                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>

                    </LinearLayout>
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:layout_marginBottom="20dp"
                        android:layout_marginStart="5dp"
                        android:layout_marginEnd="5dp"
                        android:background="#F8F7F6">

                        <Button
                            android:id="@+id/backbtn"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="right"
                            android:layout_marginEnd="5dp"
                            android:layout_marginStart="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            android:background="#3149CD"
                            android:text="Back" />
                        <Button
                            android:id="@+id/tnxtbtn"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="right"
                            android:layout_marginEnd="5dp"
                            android:layout_marginStart="300dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            android:background="#3149CD"
                            android:text="Next"
